#data-content{
    position: absolute;
    top: 0;
    bottom: 0;
    /* left: 340px; */
    left: 18%;
    right: 0;
    overflow: hidden;
}

#data-content .toolbar{
    height: 40px;
    line-height: 62px;
    white-space: nowrap;
    position: relative;
}

#data-content .toolbar .pull-down-list{
    display: inline-block;
}

#data-content .toolbar .btn{
    position: absolute;
    top: 14px;
    right: 50px;
    width: 80px;
    height: 26px;
    line-height: 26px;
}

.data-box{
    position: absolute;
    top: 50px;
    left: 20px;
    right: 20px;
    bottom: 10px;
    overflow: hidden;
}

#history-div{
    position: absolute;
    width: 1200px;
    height: 360px;
    border: 1px solid rgb(83, 187, 235);
    background: rgba(0, 0, 0, 0.9);
    z-index: 99;
    display: none;
}

#history-div .title{
    background: rgb(83, 187, 235);
    margin-top: 0;
    height: 30px;
    line-height: 30px;
    border-bottom: 1px solid rgb(83, 187, 235);
    text-indent: 10px;
    color: #000;
    font-weight: bold;
}

#history-div .title i{
    position: absolute;
    top: 8px;
    right: 8px;
    cursor: pointer;
}

#history-div .title i:hover{
    color: #333;
}

#history-div .box{
    position: absolute;
    top: 50px;
    bottom: 20px;
    left: 20px;
    right: 20px;
    background: rgba(255, 255, 255, 0.1)
}

@media screen and (max-width: 1600px){
    /* #data-content{
        left: 320px;
    } */

    #type-list{
        margin-left: 118px;
    }
}